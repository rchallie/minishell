		if (*(entry + 1) == '\0' && (simple_q || double_q))
		{
			char *test = edit_line();
			ft_printf("Test = %s\n", test);
			int entry_advencement = entry - save_entry;
			entry = ft_strjoin(save_entry, test);
			ft_printf("Entry adven = %d\nEntry = %s\n", entry_advencement, entry);
			save_entry = entry;
			entry += entry_advencement;
			continue;
		}
